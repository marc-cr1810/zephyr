# test_internal.zephyr - Test internal keyword parsing

# Regular function (should parse)
func public_function() {
    return "This is public"
}

# Internal function (should parse with new parser)
internal func private_function() {
    return "This is internal"
}

# Regular constant (should parse)
const PUBLIC_CONST = 42

# Internal constant (should parse with new parser)
internal const PRIVATE_CONST = 99

# Internal class (should parse with new parser)
internal class PrivateClass {
    func init() {
        this.value = "private"
    }

    func get_value() {
        return this.value
    }
}

# Test the functions
print("Public function:", public_function())
print("Private function:", private_function())
print("Public constant:", PUBLIC_CONST)
print("Private constant:", PRIVATE_CONST)

# Test the class
private_obj = PrivateClass()
print("Private class value:", private_obj.get_value())

print("Internal parsing test completed successfully!")
