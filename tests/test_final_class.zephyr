# Test final class modifier

class Animal {
    name : string

    func init(name : string) {
        this.name = name
    }

    func speak() {
        print("Animal makes a sound")
    }
}

final class Cat : Animal {
    func init(name : string) {
        super.init(name)
    }

    func speak() {
        print(this.name + " meows!")
    }
}

# Test that final class works
cat = Cat("Whiskers")
cat.speak()

