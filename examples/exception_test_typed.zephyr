# Typed Catch Test to Debug Issues

print("Testing typed exception catching...")

# Test 1: Basic typed catch
try {
    print("About to throw IndexError...")
    throw IndexError("Index out of bounds")
    print("This should not be reached")
} catch (e as IndexError) {
    print("Caught specific IndexError:", e.message)
    print("Exception type:", e.type)
} catch (e) {
    print("This catch should not be reached for IndexError")
}

print()

# Test 2: Multiple typed catches
try {
    print("About to throw TypeError...")
    throw TypeError("Type mismatch")
    print("This should not be reached")
} catch (e as IndexError) {
    print("This should not be reached for TypeError")
} catch (e as TypeError) {
    print("Caught specific TypeError:", e.message)
    print("Exception type:", e.type)
} catch (e) {
    print("This catch should not be reached for TypeError")
}

print()

# Test 3: Generic catch for unmatched type
try {
    print("About to throw ValueError...")
    throw ValueError("Invalid value")
    print("This should not be reached")
} catch (e as IndexError) {
    print("This should not be reached for ValueError")
} catch (e as TypeError) {
    print("This should not be reached for ValueError")
} catch (e) {
    print("Caught unmatched exception:", e.message)
    print("Exception type:", e.type)
}

print("Test completed.")
