# Chainable calculator for fluent interface
class ChainableCalculator {
    func init(value) {
        this.value = value
    }

    func add(x) {
        this.value = this.value + x
        return this
    }

    func multiply(x) {
        this.value = this.value * x
        return this
    }

    func subtract(x) {
        this.value = this.value - x
        return this
    }

    func get_value() {
        return this.value
    }

    func reset() {
        this.value = 0
        return this
    }
}

# Test method chaining
